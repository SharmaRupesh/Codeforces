import java.util.*;

public class HelloWorld
{
     public static void main(String []args)
     {
         Scanner sc = new Scanner(System.in);
         
         long n = sc.nextLong();
         int k  = sc.nextInt();
         
         while(k>0)
         {
             if(n%10==0)
             {
                 n = n/10;
             }
             else
             {
                 n--;
             }
             k--;
         }
         System.out.println(n);
         
     }
}
